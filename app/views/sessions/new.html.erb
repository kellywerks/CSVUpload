<h2>Log in</h2>

<%= form_tag sessions_path do %>
  <div class="row">
    <%= label_tag :email %>
    <%= text_field_tag :email %>
  </div>
  <div class="row">
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class= "actions"><%= submit_tag "Log In" %></div>
<% end %>
